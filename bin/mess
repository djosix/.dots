#!/bin/bash

{
    echo -n ''
    {
        echo -n 'Me55'
        {
            echo -n 'Me55Me55'
            {
                echo -n 'Me55Me55Me55'
                {
                    echo -n 'Me55Me55Me55Me55'
                    for s in $@; do echo "Me55Me55Me55Me55Me55$s"; done
                    echo -n 'Me55Me55Me55Me55'
                } | md5sum | rev | xxd -r -p
                echo -n 'Me55Me55Me55'
            } | sha1sum | rev | xxd -r -p
            echo -n 'Me55Me55'
        } | md5sum | rev | xxd -r -p
        echo -n 'Me55'
    } | sha1sum | rev | xxd -r -p
    echo -n ''
} | base64 | sed 's/[^a-zA-Z0-9]//g'

